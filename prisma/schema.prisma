// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Usuario{
  id Int @id @default(autoincrement())
  nome String
  email String @unique
  senha String 
  foto String?
  cargo String
}

model Artista{
  id Int @id @default(autoincrement())
  nome String
  foto String?
  num_streams Int
}